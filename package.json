  "description": "",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "parcel app/index.html -d build/local",
    "build": "run-p build:*",
    "build:webapp": "parcel build app/index.html -d build/dist",
    "build:server": "tsc --project server/tsconfig.json",
    "prebuild": "rimraf build",
    "deploy": "git push heroku master -f",
    "lint:fix": "eslint . . --ext .js,.ts,.tsx --fix",
    "format": "prettier '**/*.{less,ts,tsx}' --ignore-path='.eslintignore'",
    "format:write": "npm run lint:fix && npm run format -- --write",
    "verify": "run-p verify:*",
    "verify:audit": "npm audit --registry=https://registry.npmjs.org/ --audit-level=moderate",
    "verify:types": "tsc --noEmit",
    "verify:format": "prettier '**/*.{less,ts,tsx}' --ignore-path='.eslintignore' -l",
    "verify:lint": "eslint . . --ext .js,.ts,.tsx"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.5.0",
    "less": "^3.11.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "engines": {
    "node": "13.6.x"
  }
